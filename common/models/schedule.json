{
  "name": "Schedule",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "foreignKeys": {
    "customerId": {
      "name": "customerId",
      "foreignKey": "customerId",
      "entityKey": "id",
      "entity": "Customer"
    },
    "userId": {
      "name": "userId",
      "foreignKey": "userId",
      "entityKey": "id",
      "entity": "Users"
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "start_date_time": {
      "type": "date",
      "required": true,
      "defaultFn": null
    },
    "end_date_time": {
      "type": "date",
      "required": true,
      "defaultFn": null
    }
  },
  "validations": [],
  "relations": {
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customerId",
      "options": {
        "nestRemoting": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "Users",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
