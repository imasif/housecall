{
    "name": "Grouping",
    "plural": "groupings",
    "base": "PersistedModel",
    "idInjection": false,
    "options": {
      "validateUpsert": true
    },
    "scope": {
      "include": [
        {
          "relation": "estimates"
        },
        {
          "relation": "jobs"
        },
        {
          "relation": "invoices"
        }
      ]
    },
    "properties": {
      "id": {
        "type": "String",
        "id": true,
        "required": true,
        "defaultFn": "uuid"
      },
      "name": {
        "type": "string",
        "required": true
      }
    },
    "validations": [],
    "relations": {
      "estimates": {
          "type": "hasMany",
          "model": "Estimate",
          "foreignKey": "groupingId",
          "options": {
            "nestRemoting": true
          }
      },
      "jobs": {
          "type": "hasMany",
          "model": "Job",
          "foreignKey": "groupingId",
          "options": {
            "nestRemoting": true
          }
      },
      "invoices": {
          "type": "hasMany",
          "model": "Invoice",
          "foreignKey": "groupingId",
          "options": {
            "nestRemoting": true
          }
      }
    },
    "acls": [
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "$everyone",
        "permission": "DENY"
      },
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "admin",
        "permission": "ALLOW"
      }
    ],
    "methods": {}
  }
  