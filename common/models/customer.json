{
  "name": "Customer",
  "plural": "customers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "home_phone": {
      "type": "string",
      "required": true
    },
    "mobile_phone": {
      "type": "string",
      "required": true
    },
    "work_phone": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string",
      "required": true
    },
    "job_title": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "address": {
      "type": "embedsMany",
      "model": "Address",
      "options": {
        "forceId": true,
        "validate": true,
        "autoId": true
      },
      "through": "Address"
    },
    "email": {
      "type": "embedsMany",
      "model": "Email",
      "options": {
        "forceId": true,
        "validate": true,
        "autoId": true
      },
      "through": "Email"
    },
    "phone": {
      "type": "embedsMany",
      "model": "Phone",
      "options": {
        "forceId": true,
        "validate": true,
        "autoId": true
      },
      "through": "Phone"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
