{
  "name": "Invoice",
  "plural": "invoices",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "foreignKeys": {
    "customerId": {
      "name": "customerId",
      "foreignKey": "customerId",
      "entityKey": "id",
      "entity": "Customer"
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "type": {
      "type": "string",
      "required": true
    },
    "sub_total": {
      "type": "number",
      "required": true
    },
    "total": {
      "type": "number",
      "required": true
    },
    "notes": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "grouping": {
      "type": "hasOne",
      "model": "Grouping",
      "foreignKey": "id",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customerId",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "user": {
      "type": "belongsTo",
      "model": "Users",
      "foreignKey": "userId",
      "options": {
        "nestRemoting": true,
        "disableInclude": true
      }
    },
    "servicelist": {
      "type": "embedsMany",
      "model": "Servicelist",
      "options": {
        "forceId": true,
        "validate": true,
        "autoId": true
      }
    },
    "materiallist": {
      "type": "embedsMany",
      "model": "Materiallist",
      "options": {
        "forceId": true,
        "validate": true,
        "autoId": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
